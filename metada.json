
{
	"id": "com.sap.sample.coloredboxa",
	"version": "2.0.1",
	"name": "Colored Box2 ",
	"description": "A colored box2",
	"newInstancePrefix": "AColoredBox",
	"icon": "https://www.sample.com/customwidgets/coloredbox/icon.png",
	"vendor": "SAP",
	"eula": "",
	"license": "",
	"webcomponents": [
	{	
	"kind": "main",
	"tag": "com-sap-sample-coloredboxa",
	"url": "https://fabiopereira123.github.io/SACC/webcomponent.js",
	"integrity": "",
	"ignoreIntegrity": true
	},
	{
	"kind": "styling",
	"tag": "com-sap-sample-coloredbox-stylinga",
	"url": "https://fabiopereira123.github.io/SACC/styling.js",
	"integrity": "",
	"ignoreIntegrity": true
	},
	{
	"kind": "builder",
	"tag": "com-sap-sample-coloredbox-builder",
	"url": "https://fabiopereira123.github.io/SACC/builder.js",
	"integrity": "",
	"ignoreIntegrity": true
	}
	],
	
	"properties": {
	"color": {
	"type": "string",
	"description": "Background color",
	"default": "red"
	},
	"opacity": {
	"type": "number",
	"description": "Opacity",
	"default": 1
	},
	"width": {
	"type": "integer",
	"default": 200
	},
	"height": {
	"type": "integer",
	"default": 120
	}
	},
	"methods": {
	"setColor": {
	"description": "Sets the background color.",
	"parameters": [
	{
	"name": "newColor",
	"type": "string",
	"description": "The new background color"
	}
	],
	"body": "this.color = newColor;"
	},
	"getColor": {
	"returnType": "string",
	"description": "Returns the background color.",
	"body": "return this.color;"
	}
	},
	"events": {
	"onClick": {
	"description": "Called when the user clicks the Colored Box."
	}
	}
